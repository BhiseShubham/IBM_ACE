

CREATE DATABASE MODULE DatabaseNode_MF_Database
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		 CALL CopyEntireMessage();
		RETURN TRUE;
	END;
	CREATE PROCEDURE CopyEntireMessage() BEGIN
		DECLARE AB REFERENCE TO Root.XMLNSC.root;
		DECLARE NAME CHARACTER;
		DECLARE ID INTEGER;
		DECLARE PLACE CHARACTER;
		
		SET  NAME= AB.NAME;
		SET ID = AB.ID;
		SET PLACE =  AB.PLACE;
		INSERT INTO Database.STUDENT (NAME,ID,PLACE) VALUES (NAME,ID,PLACE);
		
	END;
END MODULE;


